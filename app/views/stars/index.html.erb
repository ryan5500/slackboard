<h1>Listing stars</h1>

<table>
  <thead>
    <tr>
      <th>Post type</th>
      <th>Ts</th>
      <th>User</th>
      <th>Text</th>
      <th>Ts date</th>
      <th>Ch id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= line_chart @chart_data, height: "800px" %>
    <% @stars.each do |star| %>
      <tr>
        <td><%= star.post_type %></td>
        <td><%= star.ts %></td>
        <td><%= star.user %></td>
        <td><%= star.text %></td>
        <td><%= star.ts_date %></td>
        <td><%= star.ch_id %></td>
        <td><%= link_to 'Show', star%></td>
        <td><%= link_to 'Edit', edit_star_path(star) %></td>
        <td><%= link_to 'Destroy', star, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
