<% if local_assigns[:discrete] and discrete %>
    <%= line_chart chart_input_data, discrete: true, height: '500px' ,library: { backgroundColor: '#272B30', annotations: { textStyle: { color: '#000', auraColor: '#000', opacity: 1.0 } } } %>
<% else %>
    <%= line_chart chart_input_data, height: '500px' ,library: { backgroundColor: '#272B30', annotations: { textStyle: { color: '#000', auraColor: '#000', opacity: 1.0 } } } %>
<% end %>
